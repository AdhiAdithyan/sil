  "javascript": "frappe.ready(function() {\n    document.querySelector('.btn-new-customer').addEventListener('click', function () {\n        window.location.href = '/new-customer/new';  // This will instantly redirect\n    });\n});\n\n// On your web form page\nfrappe.ready(function() {\n    // Hide form fields initially\n    const style = document.createElement('style');\n    style.textContent = '.frappe-control { opacity: 0; }';\n    document.head.appendChild(style);\n    \n    // Once page loads, gradually show fields\n    setTimeout(() => {\n        const fields = document.querySelectorAll('.frappe-control');\n        fields.forEach((field, index) => {\n            setTimeout(() => {\n                field.style.opacity = '1';\n                field.style.transition = 'opacity 0.2s';\n            }, index * 100);\n        });\n    }, 0);\n});",
