  "main_section_html": "<html>\n<head>\n    <title>Inventory Management</title>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css\">\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            background: linear-gradient(to bottom, #e0f7fa, #ffffff);\n            margin: 0;\n            padding: 0;\n        }\n        .header {\n            background-color: #333;\n            color: white;\n            padding: 10px 20px;\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n        }\n        .header .title {\n            font-size: 20px;\n        }\n        .header .user-info {\n            font-size: 14px;\n        }\n        .header .user-info a {\n            color: white;\n            text-decoration: none;\n            margin-left: 10px;\n        }\n        .nav {\n            background-color: #00695c;\n            padding: 10px 20px;\n        }\n        .nav a {\n            color: white;\n            text-decoration: none;\n            margin-right: 15px;\n            font-size: 14px;\n        }\n        .container {\n            padding: 20px;\n            margin-bottom: 60px; /* Added bottom margin */\n        }\n        .search-section, .form-section {\n            background-color: white;\n            border: 1px solid #ccc;\n            padding: 20px;\n            margin-bottom: 20px;\n        }\n        .search-section select, .search-section input, .search-section button {\n            margin-right: 10px;\n            padding: 5px;\n            font-size: 14px;\n        }\n        .form-section input[type=\"text\"], .form-section select, .form-section input[type=\"file\"] {\n            width: calc(33.33% - 20px);\n            margin-right: 10px;\n            padding: 5px;\n            font-size: 14px;\n            margin-bottom: 10px;\n        }\n        .form-section input[type=\"checkbox\"] {\n            margin-right: 5px;\n        }\n        .form-section .attributes {\n            display: flex;\n            align-items: center;\n        }\n        .form-section .attributes select {\n            width: calc(50% - 20px);\n            margin-right: 10px;\n        }\n        .form-section .attributes button {\n            padding: 5px 10px;\n            font-size: 14px;\n        }\n        .form-section .buttons {\n            margin-top: 20px;\n        }\n        .form-section .buttons button {\n            padding: 10px 20px;\n            font-size: 14px;\n            margin-right: 10px;\n        }\n        .footer {\n            background-color: #004d40;\n            color: white;\n            text-align: center;\n            padding: 10px;\n            position: fixed;\n            width: 100%;\n            bottom: 0;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"header\">\n        <div class=\"title\">Inventory Management</div>\n        <div class=\"user-info\">\n            <span>Welcome AppAdmin</span>\n            <a href=\"#\">Sign Out</a>\n        </div>\n    </div>\n    <div class=\"nav\">\n        <a href=\"#\">Home</a>\n        <a href=\"#\">Inventory Settings</a>\n        <a href=\"#\">Warehouse settings</a>\n        <a href=\"#\">Purchase Settings</a>\n        <a href=\"#\">Manage Purchases</a>\n        <a href=\"#\">Stock Transfer</a>\n        <a href=\"#\">Inventory Reports</a>\n        <a href=\"#\">Privileges</a>\n    </div>\n    <div class=\"container\">\n        <div class=\"search-section\">\n            <select>\n                <option>Search In</option>\n            </select>\n            <input type=\"text\" placeholder=\"Search Type\">\n            <select>\n                <option>Not Approved</option>\n            </select>\n            <button>Search</button>\n        </div>\n        <div class=\"form-section\">\n            <div>\n                <input type=\"text\" placeholder=\"Stock Item Code\">\n                <input type=\"text\" placeholder=\"Stock Item Name\">\n                <select>\n                    <option>Select Stock Type</option>\n                </select>\n            </div>\n            <div>\n                <select>\n                    <option>Select Category</option>\n                </select>\n                <select>\n                    <option>Select Sub Category</option>\n                </select>\n                <select>\n                    <option>Select Unit</option>\n                </select>\n            </div>\n            <div>\n                <input type=\"text\" placeholder=\"Stock MRP\" value=\"0\">\n                <input type=\"text\" placeholder=\"Minimum Sale Quantity\">\n                <input type=\"file\">\n            </div>\n            <div>\n                <input type=\"text\" placeholder=\"HSN Code\">\n                <select>\n                    <option>Select Brand</option>\n                </select>\n                <input type=\"text\" placeholder=\"Alias Name\" value=\"0\">\n            </div>\n            <div>\n                <input type=\"checkbox\" checked> Allow Stock Assembly\n                <input type=\"checkbox\" checked> Allow Counter Sale\n                <input type=\"checkbox\" checked> Is Active Stock\n            </div>\n            <div class=\"attributes\">\n                <select>\n                    <option>Select Attribute Type</option>\n                </select>\n                <select>\n                    <option>Select Attribute</option>\n                </select>\n                <button>+</button>\n            </div>\n            <div class=\"buttons\">\n                <button>Submit</button>\n                <button>Cancel</button>\n            </div>\n        </div>\n    </div>\n    <div class=\"footer\">\n        Copyright Â© Softland India Ltd.\n    </div>\n</body>\n</html>",
